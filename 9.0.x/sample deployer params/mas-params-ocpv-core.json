            "*.edgeconfigapi.iot.inst1.apps",
            "*.monitor.inst1.apps",
            "*.api.monitor.inst1.apps",
            "*.manage.inst1.apps",
            "*.health.inst1.apps",
            "*.api.optimizer.inst1.apps"
        ]
    }
}